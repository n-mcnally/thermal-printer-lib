    {
      "path": "dist/thermal-printer-lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.2",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
