  "repository": {
    "type": "git",
    "url": "https://github.com/n-mcnally/thermal-printer-lib.git"
  },
  "module": "dist/thermal-printer-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/thermal-printer-lib.cjs.production.min.js",
